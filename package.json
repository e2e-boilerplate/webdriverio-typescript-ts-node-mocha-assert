{
  "name": "webdriverio-es6modules-typescript-tsnode-mocha-chai-sync",
  "version": "1.0.0",
  "description": "WebdriverIO (sync) end-to-end test automation boilerplate project. Using TypeScript, ES6 modules, ts-node, mocha, and chai",
  "scripts": {
    "pretty": "npx prettier --no-config './spec/*.ts' --write",
    "lint": "npx tslint -c tslint.json './spec/*.ts'",
    "test": "npx wdio wdio.conf.js",
    "test-ci": "npx wdio wdio-ci.conf.js"
  },
  "keywords": [
    "webdriverio",
    "typescript",
    "es6 modules",
    "ts-node",
    "mocha",
    "sync",
    "boilerplate",
    "example"
  ],
  "author": "Girma Nigusse",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.26",
    "@wdio/cli": "5.18.6",
    "@wdio/dot-reporter": "5.18.6",
    "@wdio/local-runner": "5.18.6",
    "@wdio/mocha-framework": "5.18.6",
    "@wdio/sync": "5.18.6",
    "chromedriver": "79.0.2",
    "husky": "4.2.1",
    "lint-staged": "10.0.6",
    "mocha": "7.0.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.7.5",
    "wdio-chromedriver-service": "5.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
